{
    "name": "cronixweb/bigcommerce-api-client",
    "description": "Big Commerce REST API client for Laravel by Cronix Web.",
    "type": "library",
    "version": "1.0.0",
    "authors": [
        {
            "name": "Cronix Web",
            "email": "developer@cronixweb.com"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": "^8.1",
        "ext-json": "*",
        "illuminate/http": ">=10.0",
        "illuminate/support": ">=10.0",
        "spatie/laravel-package-tools": "^1.14"
    },
    "autoload": {
        "psr-4": {
            "CronixWeb\\BigCommerceAPI\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "CronixWeb\\BigCommerceAPI\\BigCommerceServiceProvider"
            ]
        }
    }
}
